                  <xs:element name="stddev" minOccurs="1" maxOccurs="1" default="0.0" type="xs:double">
                    <xs:annotation>
                      <xs:documentation xml:lang="en">For type "gaussian," the standard deviation of the Gaussian distribution from which noise values are drawn.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:all>
              </xs:complexType>
            </xs:element>
          </xs:all>
        </xs:complexType>
      </xs:element>
      <xs:element name="rfidtag" minOccurs="0" maxOccurs="1" type="xs:string"/>
