            <xs:element name="min" minOccurs="1" maxOccurs="1" default="0" type="xs:double">
              <xs:annotation>
                <xs:documentation xml:lang="en">Minimum range</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="max" minOccurs="1" maxOccurs="1" default="1.0" type="xs:double">
              <xs:annotation>
                <xs:documentation xml:lang="en">Max range</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="radius" minOccurs="1" maxOccurs="1" default="0.5" type="xs:double">
              <xs:annotation>
                <xs:documentation xml:lang="en">Radius of the sonar cone at max range. This parameter is only used if geometry is "cone".</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:all>
        </xs:complexType>
      </xs:element>
      <xs:element name="transceiver" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="en">These elements are specific to a wireless transceiver.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:annotation>
            <xs:documentation xml:lang="en">These elements are specific to a wireless transceiver.</xs:documentation>
          </xs:annotation>
          <xs:all>
            <xs:element name="essid" minOccurs="1" maxOccurs="1" default="wireless" type="xs:string">
              <xs:annotation>
                <xs:documentation xml:lang="en">Service set identifier (network name)</xs:documentation>
              </xs:annotation>
            </xs:element>
