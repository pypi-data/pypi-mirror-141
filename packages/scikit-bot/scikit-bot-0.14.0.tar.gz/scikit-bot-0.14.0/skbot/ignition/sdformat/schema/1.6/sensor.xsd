            <xs:element name="min_frequency" minOccurs="1" maxOccurs="1" default="2412" type="xs:double">
              <xs:annotation>
                <xs:documentation xml:lang="en">Only a frequency range is filtered. Here we set the lower bound (MHz).</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="max_frequency" minOccurs="1" maxOccurs="1" default="2484" type="xs:double">
              <xs:annotation>
                <xs:documentation xml:lang="en">Only a frequency range is filtered. Here we set the upper bound (MHz).</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="gain" minOccurs="1" maxOccurs="1" default="2.5" type="xs:double">
              <xs:annotation>
                <xs:documentation xml:lang="en">Specifies the antenna gain in dBi</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="power" minOccurs="1" maxOccurs="1" default="14.50" type="xs:double">
              <xs:annotation>
                <xs:documentation xml:lang="en">Specifies the transmission power in dBm</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="sensitivity" minOccurs="1" maxOccurs="1" default="-90" type="xs:double">
              <xs:annotation>
                <xs:documentation xml:lang="en">Mininum received signal power in dBm</xs:documentation>
              </xs:annotation>
